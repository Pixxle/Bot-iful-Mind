{
  "name": "bot-iful-mind",
  "version": "1.0.0",
  "description": "Modular Telegram bot with tool integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx scripts/dev.ts",
    "setup": "tsx scripts/setup.ts",
    "set-webhook": "tsx scripts/set-webhook.ts",
    "clear-webhook": "tsx scripts/clear-webhook.ts",
    "build": "tsc",
    "start": "node dist/index.js",
