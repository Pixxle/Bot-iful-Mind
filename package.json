    "cheerio": "^1.1.0",
    "dotenv": "^16.3.1",
    "openai": "^4.26.0",
    "telegraf": "^4.15.3",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vercel/node": "^5.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "ngrok": "^5.0.0-beta.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.35.0"
  }
